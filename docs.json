{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "varg",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "SDK",
            "pages": [
              "sdk/index",
              "sdk/components",
              "sdk/models",
              "sdk/cli"
            ]
          },
          {
            "group": "Templates",
            "pages": [
              "templates/index",
              "templates/slideshow",
              "templates/talking-character",
              "templates/transformation"
            ]
          },
          {
            "group": "AI Agents",
            "pages": [
              "ai-agents/index",
              "ai-agents/claude-code",
              "ai-agents/cursor"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Gateway API",
            "pages": [
              "api/index",
              "api/video",
              "api/image",
              "api/speech",
              "api/music",
              "api/jobs"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/vargHQ/varg",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/varg",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://varg.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://varg.ai/dashboard"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/vaborsh",
      "github": "https://github.com/vargHQ"
    }
  }
}
